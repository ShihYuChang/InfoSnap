{
  "manifest_version": 3,
  "name": "InfoSnap",
  "description": "Access To Your Daily Info At Once",
  "version": "1.3.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": "logo.png"
  },
  "permissions": ["contextMenus"],
  "icons": {
    "16": "logo.png",
    "32": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "background": {
    "service_worker": "./background.js",
    "type": "module"
  }
}
